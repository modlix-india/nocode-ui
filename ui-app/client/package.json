{
	"name": "nocode-ui",
	"version": "2.0.0",
	"description": "UI Engine for NoCode SaaS Platform",
	"browserslist": [
		"> 0.5%",
		"last 2 versions",
		"not dead"
	],
	"scripts": {
		"start": "webpack serve --config webpack.dev.js",
		"dev": "./dev-with-watch.sh",
		"build": "webpack --config webpack.prod.js",
		"cf-local": "webpack --config webpack.prod.js --env publicUrl=https://cdn-local.modlix.com/js/dist/ ",
		"cf-dev": "webpack --config webpack.prod.js --env publicUrl=https://cdn-dev.modlix.com/js/dist/",
		"cf-stage": "webpack --config webpack.prod.js --env publicUrl=https://cdn-stage.modlix.com/js/dist/",
		"cf-prod": "webpack --config webpack.prod.js --env publicUrl=https://cdn.modlix.com/js/dist/",
		"bundle-analyze": "webpack --config webpack.prod.js --profile --json --env publicUrl=https://cdn-local.modlix.com/js/dist/ > report/stats.json && webpack-bundle-analyzer report/stats.json dist -p 1234",
		"test": "jest --watchAll",
		"coverage": "jest --coverage",
		"pretty": "prettier --write ./src || prettier --write ./__tests__"
	},
	"keywords": [
		"nocode",
		"lowcode"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier --write",
			"eslint --fix",
			"git add"
		],
		"*.{html,css,less,ejs}": [
			"prettier --write",
			"git add"
		]
	},
	"author": "Kiran Kumar Grandhi",
	"contributors": [
		"Kiran Kumar Grandhi",
		"Raja Avinash Alli",
		"Akhilesh Alli",
		"Surendhar Satyavarapu",
		"Tarun Jangra",
		"Md Shagil Nizami",
		"Himanshu",
		"Roopa Soumya Kandula",
		"Chavala Venktarao",
		"Sachin B",
		"Ronak Piraka"
	],
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.26.0",
		"@babel/preset-env": "^7.26.0",
		"@babel/preset-react": "^7.26.3",
		"@babel/preset-typescript": "^7.26.0",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
		"@tsconfig/recommended": "^1.0.8",
		"@types/jest": "^29.5.14",
		"@types/luxon": "^3.7.1",
		"@types/node": "^22.10.2",
		"@types/react": "^18.3.13",
		"@types/react-dom": "^18.3.1",
		"@typescript-eslint/eslint-plugin": "^8.17.0",
		"@typescript-eslint/parser": "^8.17.0",
		"babel-loader": "^9.2.1",
		"buffer": "^6.0.3",
		"clean-webpack-plugin": "^4.0.0",
		"css-loader": "^7.1.2",
		"eslint": "^9.16.0",
		"eslint-plugin-react": "^7.37.2",
		"html-webpack-plugin": "^5.6.3",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^30.2.0",
		"lint-staged": "^15.2.10",
		"prettier": "^3.4.2",
		"prettier-plugin-organize-imports": "^4.1.0",
		"process": "^0.11.10",
		"react-refresh-typescript": "^2.0.9",
		"source-map-loader": "^5.0.0",
		"style-loader": "^4.0.0",
		"ts-jest": "^29.2.5",
		"ts-loader": "^9.5.1",
		"typescript": "^5.7.2",
		"webpack": "^5.97.1",
		"webpack-bundle-analyzer": "^4.10.2",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^5.2.0",
		"webpack-manifest-plugin": "^6.0.1"
	},
	"dependencies": {
		"@fincity/kirun-js": "^3.3.0",
		"@fincity/kirun-ui": "^1.1.0",
		"@fincity/path-reactive-state-management": "^2.9.2",
		"@monaco-editor/react": "^4.6.0",
		"axios": "^1.7.9",
		"chart.js": "^4.5.1",
		"monaco-editor": "^0.51.0",
		"react": "^18.3.1",
		"react-chartjs-2": "^5.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.28.0"
	},
	"jest": {
		"transform": {
			".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"globals": {
			"window": {}
		}
	}
}
