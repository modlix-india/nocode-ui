{
	"compilerOptions": {
		"lib": ["ES2022", "DOM"],
		"jsx": "react-jsx",
		"resolveJsonModule": true,
		"module": "ESNext",
		"target": "ESNext",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"types": ["jest", "node"],
		"sourceMap": true
	}
}